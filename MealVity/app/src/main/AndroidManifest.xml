<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cenec.darash.mealvity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cenec.mealvity.mealvity.FragmentContainerActivity" />
        <activity android:name="cenec.mealvity.mealvity.LoadingActivity" />
        <activity android:name="cenec.mealvity.mealvity.ResetPasswordActivity" />
        <activity android:name="cenec.mealvity.mealvity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cenec.mealvity.mealvity.SignUpActivity" />
        <activity android:name="cenec.mealvity.mealvity.SignInActivity" />
        <activity android:name="cenec.mealvity.mealvity.MainActivity" />
    </application>

</manifest>